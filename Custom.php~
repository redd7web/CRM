$(document).ready(function () {

    var one, two, total;
    // JavaScript for form_10859
    if ($('form_10859').length) {

        $('element_28').prop('disabled', true);
        $(document).change(function () {
            one = $('element_25').val();
            two = $('element_27').val();
            total = parseFloat(one) * parseFloat(two);
            $('element_28').val(parseFloat(total).toFixed(2));
        });
    }
    

