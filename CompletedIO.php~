        if(isset($_POST['inbound_submit'])){
            unset($ib);
            foreach($_POST as $name=>$value){
                switch($name){
                    case "outbound_from":
                        if(strlen($value)>0 && $value !=' '){
                            $ib[] = " DATE(date_created) >='$value'";
                        }
                    break;
                    case "outbound_to":
                        if(strlen($value)>0 && $value !=' '){
                             $ib[] = " DATE(date_created) <='$value'";
                        }
                    break;
                }
            }
            
            if(!empty($ib)>0){
                $string = " AND ".implode(" AND ", $ib);
            }
            //echo "SELECT * FROM Inetforms.ap_form_43256 WHERE element_1 = 4 $string";
            $outbound = $db->query("SELECT * FROM Inetforms.ap_form_43256 WHERE element_1 = 4 $string");
        }else{
            $outbound = $db->query("SELECT * FROM Inetforms.ap_form_43256 WHERE element_1 = 4");
        }
    
